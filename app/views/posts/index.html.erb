<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>
    <% if session[:user_id] %>
        <%= form_for @post, url: "/posts/create" do |f| %>
            <%= f.label :title%>
            <%= f.text_field :title %>
            <%= f.label :content%>
            <%= f.password_field :content%>
            <%= f.submit "post" %>
        <% end %>
      <% @posts.each do |post| %>
　　　<p><%=link_to post.title , '/posts/#{post.id}/edit'%></p>
     <% end %>
    <% else %>
    <a href="/sessions/new">ログインしてください。</a>
    <% end %>
